	END IF;
	
	
	--SET refSegment.ns6:"OBR.7.ObservationDateTime".ns6:"TS.1" = COALESCE(refParameter.date,CAST (CURRENT_TIMESTAMP AS CHARACTER FORMAT 'YYYYMMddHHmmss'));
	--SET refSegment.ns6:"OBR.8.ObservationEndDateTime";
