			SET refPatient.surgeryDate = CAST (CAST (refHL7.ns6:"PV2.46.PatientStatusEffectiveDate" AS TIMESTAMP FORMAT 'YYYYMMdd') AS CHARACTER FORMAT 'YYYY-MM-dd');
