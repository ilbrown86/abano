	IF NULLIF(refHL7.ns6:"DG1.20.DiagnosisIdentifier".ns6:"EI.1",'') IS NOT NULL THEN
		SET refPatient.diagnosisID = refHL7.ns6:"DG1.20.DiagnosisIdentifier".ns6:"EI.1";
	ELSE
		SET refPatient.diagnosisID = '';
	END IF;
	
END;
	
